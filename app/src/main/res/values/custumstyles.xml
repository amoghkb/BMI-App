<?xml version="1.0" encoding="utf-8"?>


<resources>
    
    
    <style name="alltext">
        <item name="background">@android:color/transparent </item>
        <item name="android:textAlignment">center</item>
        <item name="hintTextColor">#595151</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">inter_medium</item>

    </style>

    <style name="customButton">
        <item name="background">@android:color/transparent</item>
        <item name="strokeColor">@color/black</item>
        <item name="backgroundTint">@android:color/transparent</item>
        <item name="shapeAppearance">@id/rectangles</item>
        <item name="android:textAllCaps">false</item>
        <item name="strokeWidth">4dp</item>
    </style>

    <style name="newbutton">
        <item name="background">@android:color/transparent</item>
        <item name="strokeColor">#000000</item>
        <item name="android:textColor">#8AAAE5</item>
        <item name="backgroundTint">@android:color/transparent</item>
        <item name="shapeAppearance">@id/rectangles</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">20dp</item>
        <item name="strokeWidth">2dp</item>
    </style>
    

</resources>